-.Header {
+:root {
+  --terminalSize: 100%;
+}
+
+html, body {
+  margin: 0;
+  padding: 0;
+  width: 100%;
+  height: 100%;
+  display: flex;
+}
+
+Header {
   @apply text-black text-center dark:text-white font-bold text-4xl font-BreezeHeader px-5;
 }
 
@@ -8,4 +20,62 @@
 
 .Button {
   @apply text-white font-bold text-2xl font-BreezeHeader rounded-xl border-8 border-blue-600 bg-blue-600;
+}
+
+.main {
+  position: absolute;
+  background-color: #16191f;
+  width: 100%;
+  height: 100vh;
+  display: flex;
+  align-items: center;
+  justify-content: center;
+}
+
+@keyframes terminalOpen {
+  0% {transform: scale(.3);}
+  50% {transform: scale(1.05);}
+  75% {transform: scale(.95);}
+  100% {transform: scale(1);}
+}
+
+.terminal {
+  background-color: #313333;
+  margin: 0 auto;
+  width: 50%;
+  height: 50%;
+  border-radius: 5px;
+  box-shadow: 5px 20px 20px 2px #000000;
+  text-align: center;
+  animation-name: terminalOpen;
+  animation-duration: 1s;
+}
+
+.tabs {
+  height: 10%;
+  width: 100%;
+  margin: 10px;
+  display: flex;
+  align-items: center;
+  top: 0;
+}
+
+.tab {
+  background-color: #595959;
+  height: 90%;
+  width: 25%;
+  border-radius: 10px;
+}
+
+.tab:hover {
+  background-color: #4b4b4b;
+}
+
+.terminalField {
+  background-color: #141618;
+  height: 90%;
+  width: 100%;
+  position: relative;
+  border-radius: 0 0 5px 5px;
+  bottom: 0;
 }
